mixin header()
  header.header  
    .header__logo
      +logo()
    .header__block(id='js-header-block')
      .header__block__navigation
        +navigation()
      .header__block__social
        +social()
    .header__burger(id='js-header-burger') 
      .burger 
        span.line
        span.line
        span.line
